{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "job1"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                800
            ]
        },
        "equity": {
            "type": "string",
            "default": "",
            "title": "The equity Schema",
            "examples": [
                "0.25"
            ]
        }
    },
    "examples": [
        {
            "title": "job1",
            "salary": 800,
            "equity": "0.25"
        }
    ]
}